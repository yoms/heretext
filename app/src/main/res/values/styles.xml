<resources>

    <!-- Application theme. -->
    <style name="HereTextTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/HereTextActionBar</item>
    </style>

    <style name="HereTextActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:icon">@drawable/ic_launcher_white</item>
        <item name="android:background">@color/contact</item>
        <item name="android:titleTextStyle">@style/ActionBarTitleText</item>
    </style>
    <!-- ActionBar title text -->
    <style name="ActionBarTitleText"
        parent="android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@color/contactText</item>
    </style>

    <color name="user">#FAFAFA</color>
    <color name="contact">#c41915</color>
    <color name="userText">#000000</color>
    <color name="contactText">#FFFFFF</color>
    <color name="listBackground">#eeeeee</color>

</resources>